if [ "${CONTAINER_ENABLE_SCHEDULING,,}" = "true" ]; then
    if [ -f "/container/base/defaults/scheduling/${CONTAINER_SCHEDULING_BACKEND,,}" ]; then
        source /container/base/defaults/scheduling/${CONTAINER_SCHEDULING_BACKEND,,}
    fi
fi
